@{
    var pageTitle = TempData["PageTitle"] as string;
    var pageDescription = TempData["PageDescription"] as string;
}
<div class="row page-titles">
    <div class="col-md-5 col-8 align-self-center">
        <h3 class="text-themecolor m-b-0 m-t-0">@pageTitle</h3>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a>@pageTitle</a></li>
            <li class="breadcrumb-item active">@pageDescription</li>
        </ol>
    </div>
    <div class="col-md-7 col-4 align-self-center">
        <a href="https://themewagon.com/themes/material-bootstrap-4-free-admin-template/" class="btn waves-effect waves-light btn-danger pull-right hidden-sm-down">Raporları İndir</a>
    </div>
</div>

